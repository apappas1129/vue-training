{
  "name": "elearning-platform",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build",
    "server": "ts-node ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server/index.ts",
    "json-server": "node ./json-server/server.js",
    "seeder": "node ./json-server/run-faker.js > ./json-server/db.json"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "json-server": "^0.17.3",
    "lodash": "^4.17.21",
    "ofetch": "^1.1.1",
    "pinia": "^2.1.4",
    "sirv": "^2.0.2",
    "vite": "^4.0.3",
    "vite-plugin-ssr": "^0.4.131",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.7",
    "@types/json-server": "^0.14.4",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
