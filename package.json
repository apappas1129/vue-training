{
  "name": "elearning-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run serve",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run serve:ssr",
    "serve:ssr": "cross-env NODE_ENV=development ts-node ./src/server/index.ts",
    "serve:ssr:prod": "cross-env NODE_ENV=production ts-node .src/server/index.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "pinia": "^2.1.4",
    "sirv": "^2.0.3",
    "vite": "^4.3.9",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite-plugin-ssr": "^0.4.131",
    "vue-tsc": "^1.4.2"
  }
}
