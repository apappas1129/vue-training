{
  "name": "elearning-platform",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run check-redis &&  concurrently --kill-others \"npm run json-server\" \"npm run server\"",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build",
    "server": "node --experimental-specifier-resolution=node --loader ts-node/esm ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server/index.ts",
    "json-server": "node ./json-server/server.js",
    "seeder": "node ./json-server/run-faker.js > ./json-server/db.json",
    "check-redis": "node scripts/check-docker-redis.js"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/vue": "^2.2.1",
    "@tanstack/vue-table": "^8.9.3",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.2",
    "@vueuse/components": "^10.2.0",
    "@vueuse/core": "^10.2.0",
    "@vueuse/integrations": "^10.2.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "json-server": "^0.17.3",
    "lodash": "^4.17.21",
    "ofetch": "^1.1.1",
    "pinia": "^2.1.4",
    "redis": "^4.6.7",
    "sirv": "^2.0.2",
    "universal-cookie": "^4.0.4",
    "vite": "^4.0.3",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-ssr": "^0.4.131",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.7",
    "@types/json-server": "^0.14.4",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "dockerode": "^3.3.5",
    "dotenv-run-script": "^0.3.0",
    "postcss": "^8.4.24",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "type": "module"
}
